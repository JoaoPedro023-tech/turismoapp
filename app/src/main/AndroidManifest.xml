<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.turismoapp">

    <application
        android:allowBackup="true"
        android:label="TurismoApp"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Turismoapp">

        <!-- Tela principal do app (menu) -->
        <activity android:name="br.com.turismoapp.MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!-- Telas dos locais turÃ­sticos -->
        <activity android:name="br.com.turismoapp.MineiraoActivity" android:exported="true"/>
        <activity android:name="br.com.turismoapp.MercadoCentralActivity" android:exported="true"/>
        <activity android:name="br.com.turismoapp.PracaLiberdadeActivity" android:exported="true"/>
        <activity android:name="br.com.turismoapp.LagoaPampulhaActivity" android:exported="true"/>
        <activity android:name="br.com.turismoapp.CasaFiatActivity" android:exported="true"/>

    </application>

</manifest>
